<div class="well input-params">

  <label> Performer User Account Address: </label>
  <label> 0xE168E6B02409FeE106c13e410730E8b5461a8967 </label>
  <!--<input type="text" class="form-control" id="user_address"> -->

  <% if(pCases.length > 1) { %>
  <hr>
  <label> Available Process Cases: </label>
  <select id="pCase" class="form-control">
    <% for(var i = 0; i < pCases.length; i++) { %>
      <option value='<%= i %>'><%= pCases[i] %></option>
    <% } %>
  </select>
  <% } %>
  <hr>
  
  <ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#home"> Input Parameters </a></li>
  </ul>
  
  <div class="tab-content">
    <div id="home" class="tab-pane fade in active">
    <% if(inputs.length == 0 ) { %>
         <h3> NO INPUT DATA REQUIRED </h3>
    <% } else { %>
      <div class="form-group">
        <label for="name">Verification Function</label>
        <input type="text" class="form-control" id="verifying-contract" placeholder="Submit PO">
      </div>

      <div class="form-group">
        <label for="name">Smart Contract Address</label>
        <input type="text" class="form-control" id="verification-function" placeholder="0x4368f90904b1ED1FE979e36816BA523Caf92F350">
      </div>

      <div class="form-group">
        <label for="name">Attribute 1</label>
        <input type="text" class="form-control" id="Attribute1" placeholder="Please enter the parameters">
      </div>

      <div class="form-group">
        <label for="name">Attribute 2</label>
        <input type="text" class="form-control" id="Attribute2" placeholder="Please enter the parameters">
      </div>

      <div class="form-group">
        <label for="name">Attribute 3</label>
        <input type="text" class="form-control" id="Attribute3" placeholder="Please enter the parameters">
      </div>
    <% } %>
    </div>
  </div>
  
  <hr>
  
    <button type="button" class="btn btn-default" id="<%= nodeId %>_save"> Accept </button>
    <button type="button" class="btn btn-default" id="<%= nodeId %>_cancel"> Cancel </button>
  
  
  </div>
  